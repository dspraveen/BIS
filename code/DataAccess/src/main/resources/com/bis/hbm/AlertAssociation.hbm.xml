<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.bis.domain.AlertAssociation" table="alert_association" catalog="BIS">
        <id name="associationId" type="java.lang.Integer">
            <column name="Association_Id" />
            <generator class="identity" />
        </id>
        <many-to-one name="alertConfig" class="com.bis.domain.AlertConfig" column="Alert_Config_Id" cascade="none" not-null="true" lazy="false"/>
        <property name="itemId" type="java.lang.Integer">
            <column name="Item_Id" />
        </property>
        <property name="hawkerId" type="java.lang.Integer">
            <column name="Hawker_Id" />
        </property>
        <property name="vendorId" type="java.lang.Integer">
            <column name="Vendor_Id" />
        </property>
    </class>
</hibernate-mapping>
