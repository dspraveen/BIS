<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.bis.domain.ItemVendor" table="item_vendor" catalog="BIS">
        <id name="itemVendorId" type="java.lang.Integer">
            <column name="Item_Vendor_ID" />
            <generator class="identity" />
        </id>
        <property name="itemCode" type="int">
            <column name="Item_Code" not-null="true" />
        </property>
        <property name="vendorId" type="int">
            <column name="Vendor_ID" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
